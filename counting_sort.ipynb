      "source": [
        "def counting_sort(lst, max_range):\n",
        "    res = [0] * len(lst)\n",
        "    tmp = [0] * (max_range + 1)\n",
        "    for i in range(len(lst)):\n",
        "        tmp[lst[i]] += 1\n",
        "\n",
        "    k = 0\n",
        "    for i in range(len(tmp)):\n",
        "        for j in range(tmp[i]):\n",
        "            res[k] = i\n",
        "            k += 1\n",
        "    return res"
      ]
    }
  ]
}