      "source": [
        "n = int(input())\n",
        "A = list(map(int, input().split()))\n",
        "\n",
        "res_lst = [1] * len(A)\n",
        "\n",
        "for i in range(len(A)):\n",
        "    for j in range(i):\n",
        "        if A[j] < A[i]:\n",
        "            res_lst[i] += res_lst[j]\n",
        "\n",
        "print(sum(res_lst) % 10 ** 6)"
      ]
    }
  ]
