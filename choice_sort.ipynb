      "source": [
        "def choice_sort(lst):\n",
        "    for i in range(len(lst) - 1):\n",
        "        for j in range(i + 1, len(lst)):\n",
        "            if lst[i] > lst[j]:\n",
        "                lst[i], lst[j] = lst[j], lst[i]\n",
        "    return lst"
      ]
    }
  ]
