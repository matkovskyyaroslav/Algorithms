      "source": [
        "def bubble_sort(lst):\n",
        "    for i in range(1, len(lst)):\n",
        "        for j in range(len(lst) - i):\n",
        "            if lst[j] > lst[j + 1]:\n",
        "                lst[j], lst[j + 1] = lst[j + 1], lst[j]\n",
        "    return lst"
      ]
    }
  ]
}